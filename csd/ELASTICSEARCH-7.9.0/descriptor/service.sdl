{
	"name": "ELASTICSEARCH",
	"label": "NDC ElasticSearch",
	"description": "NDC Elastic Search service",
	"version": "7.9.0",
	"runAs": {
		"user": "root",
		"group": "root"
	},
	"maxInstances": 6,
	"icon": "images/icon.png",
	"compatibility": {
		"generation": 1,
		"cdhVersion": {
			"min": 5,
			"max": 6
		}
	},
	"parcel": {
		"requiredTags": ["elasticsearch"]
	},
	"roleWithExternalLinks":["ES_MASTER"],
	"roles": [{
		"name": "ES_MASTER_01",
		"label": "Es Master_01",
		"pluralLabel": "Elastic Search Master Node",
		"jvmBased": true,
		"startRunner": {
			"program": "scripts/control.sh",
			"args": ["start"],
			"environmentVariables" : {
				"CURRENT_RULE" : "es-master01",
				"ES_LOG_DIR" : "${path_logs}",
				"ES_DATA_DIR" : "${path_data}"
			}
		},
		"stopRunner" : {
			"timeout" : "90000",
			"runner" : {
				"program" : "scripts/control.sh",
				"args" : ["stop"],
				"environmentVariables" : {
					"CURRENT_RULE" : "es-master01"
				}
			}
		},
		"externalLink" : {
			"name" : "ES_MASTER",
			"label" : "Es Header",
			"url" : "http://${host}:${http_port}:/_plugin/head"
		},
		"topology" : {
			"minInstances" : "1",
			"requiresOddNumberOfInstances" : "false",
			"placementRules" : [ ]
		},
		"logging" : {
			"dir" : "${path_logs}",
			"filename" : "${cluster_name}.log",
			"modifiable" : true,
			"configName" : "log.dir",
			"loggingType" : "log4j"
		},
		"configWriter": {
			"generators": [
			{
				"filename": "elasticsearch.properties",
				"configFormat": "properties",
				"includedParams": [
					"cluster_name",
					"path_data",
					"path_logs",
					"max_local_storage_nodes",
					"initial_master_nodes",
					"discovery.seed_hosts",
					"http_port",
					"http_cors_enabled",
					"http_cors_allow_origin",
					"network_hosts"
				],
				"additionalConfigs" : [
				{
					"key" : "node.name",
					"value" : "${host}-master01"
				},
				{
					"key" : "node.roles",
					"value" : "[ master ]"
				}
				]
			},
			{
				"filename": "jvm.properties",
				"refreshable" : false,
				"configFormat": "properties",
				"includedParams": [
					"Xms",
					"Xmx",
					"UseConcMarkSweepGC",
					"CMSInitiatingOccupancyFraction",
					"UseCMSInitiatingOccupancyOnly",
					"UseG1GC",
					"G1ReservePercent",
					"InitiatingHeapOccupancyPercent",
					"HeapDumpOnOutOfMemoryError",
					"FatalErrorLogs",
					"PrintGCDetails",
					"PrintGCDateStamps",
					"PrintTenuringDistribution",
					"PrintGCApplicationStoppedTime",
					"Xloggc",
					"UseGCLogFileRotation",
					"NumberOfGCLogFiles",
					"GCLogFileSize"	
				]
			}
			]
		},
		"parameters": [
			{
				"name": "network_hosts",
				"label": "network_hosts",
				"description": "Set a custom host for HTTP",
				"configName": "network.host",
				"required": "true",
				"type": "string",
				"default": "0.0.0.0"
			},
			{
				"name": "http_port",
				"label": "HTTP port",
				"description": "Set a custom port for HTTP",
				"configName": "http.port",
				"required": "true",
				"type": "long",
				"default": "9200"
			},
			{
				"name": "path_data",
				"label": "Path to data directory",
				"description": "Path to directory where to store the data (separate multiple locations by comma)",
				"configName": "path.data",
				"required": "true",
				"type": "string",
				"pathType": "localDataDir",
				"default": "/var/lib/elasticsearch/es-master01"
			},
			{
				"name": "path_logs",
				"label": "Path to logs directory",
				"description": "Path to log files",
				"configName": "path.logs",
				"required": "true",
				"type": "path",
				"pathType": "localDataDir",
				"default": "/var/log/elasticsearch/es-master01"
			},
			{
				"name": "Xms",
				"label": "JVM Xms",
				"description": "Xms represents the initial size of total heap space",
				"configName": "Xms",
				"required": "true",
				"type": "string",
				"default": "-Xms2g"
			},
			{
				"name": "Xmx",
				"label": "JVM Xmx",
				"description": "Xms represents the maximum size of total heap space",
				"configName": "Xmx",
				"required": "true",
				"type": "string",
				"default": "-Xmx2g"
			},
			{
				"name": "UseConcMarkSweepGC",
				"label": "JVM UseConcMarkSweepGC",
				"description": "Enable ConcurrentMarkSweep (CMS)",
				"configName": "UseConcMarkSweepGC",
				"required": "false",
				"type": "string",
				"default": "8-13:-XX:+UseConcMarkSweepGC"
			},
			{
				"name": "CMSInitiatingOccupancyFraction",
				"label": "JVM CMSInitiatingOccupancyFraction",
				"description": "CMS Initiating Occupancy Fraction",
				"configName": "CMSInitiatingOccupancyFraction",
				"required": "false",
				"type": "string",
				"default": "8-13:-XX:CMSInitiatingOccupancyFraction=75"
			},
			{
				"name": "UseCMSInitiatingOccupancyOnly",
				"label": "JVM UseCMSInitiatingOccupancyOnly",
				"description": "Use CMS Initiating Occupancy Only",
				"configName": "UseCMSInitiatingOccupancyOnly",
				"required": "false",
				"type": "string",
				"default": "8-13:-XX:+UseCMSInitiatingOccupancyOnly"
