				"name": "UseG1GC",
				"label": "JVM UseG1GC",
				"description": "Enable G1GC (G1)",
				"configName": "UseG1GC",
				"required": "false",
				"type": "string",
				"default": "14-:-XX:+UseG1GC"
			},
			{
				"name": "G1ReservePercent",
				"label": "JVM G1ReservePercent",
				"description": "G1ReservePercent",
				"configName": "G1ReservePercent",
				"required": "false",
				"type": "string",
				"default": "14-:-XX:G1ReservePercent=25"
			},
			{
				"name": "InitiatingHeapOccupancyPercent",
				"label": "JVM InitiatingHeapOccupancyPercent",
				"description": "InitiatingHeapOccupancyPercent",
				"configName": "InitiatingHeapOccupancyPercent",
				"required": "false",
				"type": "string",
				"default": "14-:-XX:InitiatingHeapOccupancyPercent=30"
			},
			{
				"name": "HeapDumpOnOutOfMemoryError",
				"label": "JVM HeapDumpOnOutOfMemoryError",
				"description": "Generate a heap dump when an allocation from the Java heap fails",
				"configName": "HeapDumpOnOutOfMemoryError",
				"required": "false",
				"type": "string",
				"default": "-XX:+HeapDumpOnOutOfMemoryError"
			},
			{
				"name": "FatalErrorLogs",
				"label": "JVM FatalErrorLogs",
				"description": "specify an alternative path for JVM fatal error logs",
				"configName": "ErrorFile",
				"required": "false",
				"type": "string",
				"default": "-XX:ErrorFile=logs/hs_err_pid%p.log"
			},
			{
				"name": "PrintGCDetails",
				"label": "JVM PrintGCDetails",
				"description": "PrintGCDetails",
				"configName": "PrintGCDetails",
				"required": "false",
				"type": "string",
				"default": "8:-XX:+PrintGCDetails"
			},
			{
				"name": "PrintGCDateStamps",
				"label": "JVM PrintGCDateStamps",
				"description": "PrintGCDateStamps",
				"configName": "PrintGCDateStamps",
				"required": "false",
				"type": "string",
				"default": "8:-XX:+PrintGCDateStamps"
			},
			{
				"name": "PrintTenuringDistribution",
				"label": "JVM PrintTenuringDistribution",
				"description": "PrintTenuringDistribution",
				"configName": "PrintTenuringDistribution",
				"required": "false",
				"type": "string",
				"default": "8:-XX:+PrintTenuringDistribution"
			},
			{
				"name": "PrintGCApplicationStoppedTime",
