			{
				"name": "GCLogFileSize",
				"label": "JVM GCLogFileSize",
				"description": "GCLogFileSize",
				"configName": "GCLogFileSize",
				"required": "false",
				"type": "string",
				"default": "8:-XX:GCLogFileSize=64m"
			}
		]
